   "source": [
    "import os\n",
    "import gzip\n",
    "import shutil\n",
    "os.chdir(\"D:\\google_ngrams\\gb_12_processed_3\")\n",
    "files = os.listdir()\n",
    "for fil in files[1:]:\n",
    "    with gzip.open(fil, \"rb\") as fil_in:\n",
    "        with gzip.open(fil[:-2] + \"txt\", \"wb\") as fil_out:\n",
    "            shutil.copyfileobj(fil_in, fil_out)"
   ]
  }
